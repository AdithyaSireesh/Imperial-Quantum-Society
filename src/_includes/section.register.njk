{% for item in collections.sections %}
  {% if item.data.type === 'register' %}
    <div id="register" class="w-full flex flex-col justify-center items-center bg-gray-800 pt-16">
      <div class="self-center text-center w-full xl:w-1/3 px-4 sm:px:0">
        <h2 class="font-bold tracking-widest text-gray-200 text-4xl mb-4">{{ item.data.title }}</h2>
        <div class="font-light text-gray-400 text-xl">
          {{ item.templateContent | safe }}
        </div>
        <div class="flex flex-row w-full justify-center pt-6">
          <a class="px-10 py-2 text-gray-200 bg-blue-600 rounded-full shadow-md text-lg hover:bg-blue-800 hover:border-red" href="https://docs.google.com/forms/d/e/1FAIpQLScGqNynkyVH38Kl1-JyzyjkvSGI1_77dTriOQH9DLq44JkWhw/viewform">Sign Up</a>
        </div>
        <div>
          <a href="https://discord.gg/8n8ugCq4hR">
            <button class=" px-2 py-2 text-gray-200 bg-blue-600 rounded-full shadow-md text-lg hover:bg-blue-800 hover:border-red inline-flex items-center">
              <img class="rounded src="img/discord.png" alt="Discord" width="50px" height = "50px"/>
              <span>Discord</span>
            </button>
          </a>
        </div>
        <img class="my-12 max-w-full mx-auto" src="/img/undraw_subscriber_re_om92.svg">
      </div>
    </div>
  {% endif %}
{% endfor %}